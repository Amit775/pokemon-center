{
	"name": "pokedex-service",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/pokedex-service/src",
	"projectType": "application",
	"tags": [],
	"targets": {
		"serve": {
			"executor": "@nx/js:node",
			"defaultConfiguration": "development",
			"dependsOn": ["build"],
			"options": {
				"buildTarget": "pokedex-service:build",
				"runBuildTargetDependencies": false
			},
			"configurations": {
				"development": {
					"buildTarget": "pokedex-service:build:development"
				},
				"production": {
					"buildTarget": "pokedex-service:build:production"
				}
			}
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "apps/pokedex-service/jest.config.ts"
			}
		}
	}
}
