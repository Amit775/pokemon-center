{
	"name": "pokedex-service",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/pokedex-service/src",
	"projectType": "application",
	"tags": [],
	"targets": {
		"build": {
			"executor": "@nx/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/apps/pokedex-service",
				"main": "apps/pokedex-service/src/main.ts",
				"tsConfig": "apps/pokedex-service/tsconfig.app.json",
				"assets": ["apps/pokedex-service/src/assets"]
			},
			"configurations": {
				"development": {
					"outputPath": "dist/apps/pokedex-service"
				},
				"production": {
					"outputPath": "dist/apps/pokedex-service"
				}
			}
		},
		"serve": {
			"executor": "@nx/js:node",
			"defaultConfiguration": "development",
			"dependsOn": ["build"],
			"options": {
				"buildTarget": "pokedex-service:build",
				"runBuildTargetDependencies": false
			},
			"configurations": {
				"development": {
					"buildTarget": "pokedex-service:build:development"
				},
				"production": {
					"buildTarget": "pokedex-service:build:production"
				}
			}
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "apps/pokedex-service/jest.config.ts"
			}
		}
	},
	"implicitDependencies": ["shared-entities"]
}
