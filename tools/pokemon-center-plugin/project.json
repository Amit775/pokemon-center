{
	"name": "pokemon-center-plugin",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "tools/pokemon-center-plugin/src",
	"projectType": "library",
	"tags": [],
	"targets": {
		"build": {
			"executor": "@nx/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/tools/pokemon-center-plugin",
				"main": "tools/pokemon-center-plugin/src/index.ts",
				"tsConfig": "tools/pokemon-center-plugin/tsconfig.lib.json",
				"assets": [
					"tools/pokemon-center-plugin/*.md",
					{
						"input": "./tools/pokemon-center-plugin/src",
						"glob": "**/!(*.ts)",
						"output": "./src"
					},
					{
						"input": "./tools/pokemon-center-plugin/src",
						"glob": "**/*.d.ts",
						"output": "./src"
					},
					{
						"input": "./tools/pokemon-center-plugin",
						"glob": "generators.json",
						"output": "."
					},
					{
						"input": "./tools/pokemon-center-plugin",
						"glob": "executors.json",
						"output": "."
					}
				]
			}
		}
	}
}
